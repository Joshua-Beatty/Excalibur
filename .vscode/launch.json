{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach Karma Chrome",
            "address": "127.0.0.1",
            "port": 9334,
            "sourceMaps": true,
            "sourceMapPathOverrides": {
              "*": "${webRoot}/*"
            },
            "pathMapping": {
              "/": "${workspaceRoot}",
              "/base/": "${workspaceRoot}/"
            }
        }
    ]
}